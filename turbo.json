{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/**/.next/**",
        "apps/**/dist/**",
        "apps/desktop/renderer/**"
      ]
    },
    "build:desktop": {
      "dependsOn": ["apps/web#build"],
      "outputs": ["apps/desktop/src-tauri/target/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}